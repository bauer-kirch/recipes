{
    "manifests": {
        "bauer-kirch/pdf-bundle": {
            "manifest": {
                "bundles": {
                    "BK\\EAP\\PdfBundle\\PdfBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "PDF_API_URL": "http://browserless:3000/pdf"
                },
                "docker-compose": {
                    "docker-compose.yml": {
                        "services": [
                            "browserless:",
                            "  image: browserless/chrome:latest",
                            "  ports:",
                            "    - ${BROWSERLESS_PORT:-3000}:3000"
                        ]
                    }
                },
                "post-install-output": [
                    "  * ADMON:EAP // PdfBundle is ready."
                ]
            },
            "files": {
                "config/packages/eap_pdf.yaml": {
                    "contents": [
                        "eap_pdf:",
                        "  pdf_generation_mode: browserless",
                        "  pdf_api_url: '%env(PDF_API_URL)%'",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "fa6935e2820afac5d9648a518d455f9228da9ae1"
        }
    }
}
